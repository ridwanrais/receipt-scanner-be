                    },
                    {
                        "type": "string",
                        "description": "Source currency",
                        "name": "from",
                        "in": "query",
                        "required": true
                    },
                    {
                        "type": "string",
                        "description": "Target currency",
                        "name": "to",
                        "in": "query",
                        "required": true
                    }
